<h1>Posts#new</h1>
<p>Find me in app/views/posts/new.html.erb</p>
<%= form_with model:@post, local:true do |f| %>
  <h4>ジム名</h4>
  <%= f.text_field :gim_name %>
  
  <h4>ジム画像</h4>
  <%= f.attachment_field :gim_image %>

  <h4>ジムのレビュー</h4>
  <%= f.text_area :body %>

  <%= f.submit '投稿' %>

<% end %>
