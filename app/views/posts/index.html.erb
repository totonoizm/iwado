<h1>ジム一覧</h1>
<p>Find me in app/views/posts/index.html.erb</p>
<p>強くあれ</p>
    <%= link_to '投稿', new_post_path %>



<% @posts.each do |post| %>
    <%= link_to post_path(post.id) do %>
      <%= post.gim_name %><br>
    <% end %>
  <%= post.body %><br>
  あなたの名前　<%= post.user.username %><br>
  <%= attachment_image_tag(post, :gim_image, size: "30x30", format: 'jpeg') %>

<% end %>
